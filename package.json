{
  "name": "oddsare",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "build": "yarn run clean && yarn run tsc",
    "lint": "tslint src/**/*.{ts,tsx}",
