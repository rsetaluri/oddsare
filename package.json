    "start:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
    "start:ios": "yarn run build && concurrently -r 'yarn run watch' 'react-native run-ios'"
  },
  "dependencies": {
    "@types/jest": "^22.1.3",
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.12",
    "concurrently": "^3.5.1",
    "react": "16.2.0",
    "react-native": "0.53.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "babel-jest": "22.2.2",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.3.0",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
